        <Label Content="Номер полиса" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10"/>
        <TextBox Name="PolicyNumberTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,30,0,0" Width="300"/>
        <Button Content="Войти" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Height="30" Margin="10,70,0,0" Click="LoginButton_Click"/>
        <Button Content="Вернуться" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Height="30" Margin="150,70,0,0" Click="BackButton_Click"/>
